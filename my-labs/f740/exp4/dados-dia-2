2	


let toD = e => new Decimal(e);
let toV = e => ({ value : toD(e), error: erro_v(toD(e)) });
let toI = e => ({ value : toD(e), error: erro_i(toD(e)) });

let xv = corrente_bobina.map(toI);
let yv = tensao_bobina.map(toV);

let r = fitter.fitLin(xv, yv);

console.log(r);
