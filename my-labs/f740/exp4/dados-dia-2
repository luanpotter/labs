const labs = require ('labs');

var b = new labs.EnvBuilder();

b.variableObj({
	name: 'r',
	unit : '\\Omega'
});

const fitter = require('labs-fitter');
const Decimal = require('decimal.js');

const ERRO = new Decimal(0.05);

const erro_v = v => ERRO.times(v);
const erro_i = i => ERRO.times(i);

